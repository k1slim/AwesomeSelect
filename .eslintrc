{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "comma-dangle": 0,
    "import/no-unresolved": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "no-debugger": 1
  },
  "globals": {
  }
}
